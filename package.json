{
  "name": "prime-web",
  "description": "All applications related to PRIME, package.json with version in projects",
  "scripts": {
    "ng": "ng",
    "start": "npm run dev",
    "dev": "ng serve --host='0.0.0.0' -o",
    "build": "ng build --prod --project=prime-enrollment",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:reg": "npm run dev -- --project=prime-registration",
    "build:reg": "npm run appVersion -- -p prime-registration && npm run build -- --project=prime-registration",
    "test:reg": "npm run test -- --project=prime-registration",
    "dev:enrol": "npm run dev -- --project=prime-enrollment",
    "build:enrol": "npm run appVersion -- -p prime-enrollment && npm run build -- --project=prime-enrollment",
    "test:enrol": "npm run test -- --project=prime-enrollment",
    "prebuild": "npm run appVersion -- -p prime-enrollment",
    "build:lib": "ng build prime-core",
    "test:lib": "ng test prime-core",
    "pack:lib": "cd dist/prime-core && npm pack",
    "e2e:setup": "webdriver-manager update --versions.chrome 73.0.3683.75",
    "test-version": "npm version minor -m 'TEST build: %s'",
    "prod-version": "npm version major -m 'PROD build: %s'",
    "push-version": "git push --follow-tags",
    "appVersion": "node ./version.js",
    "webdriver-update": "webdriver-manager update"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/common": "^7.2.11",
    "@angular/compiler": "^7.2.11",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^7.2.11",
    "@angular/http": "^7.2.11",
    "@angular/platform-browser": "^7.2.11",
    "@angular/platform-browser-dynamic": "^7.2.11",
    "@angular/router": "^7.2.11",
    "@ng-select/ng-select": "^2.16.2",
    "angular2-text-mask": "^9.0.0",
    "angular2-uuid": "^1.1.1",
    "blueimp-load-image": "^2.20.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "faker": "^4.1.0",
    "font-awesome": "^4.6.3",
    "google-libphonenumber": "^3.2.2",
    "hammerjs": "^2.0.8",
    "intl-tel-input": "^15.0.2",
    "moh-common-lib": "0.0.31",
    "moment": "^2.24.0",
    "mygovbc-bootstrap-theme": "^0.4.0",
    "ng2-tel-input": "^2.0.1",
    "ngx-bootstrap": "^3.2.0",
    "ngx-mydatepicker": "^2.4.9",
    "pdfjs-dist": "^2.0.550",
    "popper.js": "^1.14.7",
    "prime-core": "file:projects/prime-core/dist/prime-core-0.0.2.tgz",
    "rxjs": "^6.4.0",
    "sha1": "^1.1.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.7",
    "@angular/compiler-cli": "^7.2.11",
    "@angular/language-service": "^7.2.11",
    "@types/faker": "^4.1.5",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^8.10.42",
    "codelyzer": "~4.2.1",
    "jasmine": "^3.3.1",
    "jasmine-core": "^2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^4.2.0",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~3.2.0"
